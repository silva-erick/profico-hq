ty_id": 3217, "admin_notes": null, "service_fee": 0.13, "has_cancelation_request": false, "can_cancel": false, "tracker_snippet_html": null, "cover_image": null, "common_id": "527a21c6-5f46-4198-9e3a-8387e477f9ed", "is_adult_content": false, "content_rating": 1, "recommended": false, "integrations": []}, "user": {"id": 1228452, "common_id": "48439914-5103-4d0a-94ec-3fa8e34c87b5", "name": "Paulo Gutemberg da Cunha Silva", "deactivated_at": null, "profile_img_thumbnail": "https://s3-sa-east-1.amazonaws.com/cdn.br.catarse/uploads/user/uploaded_image/1228452/thumb_avatar_5c20ce86-e8be-4c01-8687-e0f35968e3d1.JPG", "facebook_link": "https://www.facebook.com/wildwing.gutemberg", "twitter_username": "", "address": null, "email": null, "total_contributed_projects": 5, "total_published_projects": 2, "links": [{"id": 29944, "link": "https://www.instagram.com/streetsamuraicomic"}], "follows_count": 1, "followers_count": 17, "owner_document": null, "profile_cover_image": "https://s3-sa-east-1.amazonaws.com/cdn.br.catarse/uploads/user/cover_image/1228452/base_5ddb0283-ffec-486e-ad02-8cd9b4450bcc.png", "created_at": "2019-11-17T12:01:42.727686", "about_html": "<p>Caramelo Comics \u00e9 uma editora independente de quadrinhos dedicada a contar hist\u00f3rias originais e envolventes, escritas por Paulo Gutemberg. Sob esse selo nasceram projetos como <em>Blue Ghost</em>, <em>Ca\u00e7adores de Monstros</em> e <em>Street Samurai</em>, cada um trazendo narrativas marcantes e estilos distintos, mas todos com a identidade vibrante da editora.\n</p>\n<p>O nome \"Caramelo Comics\" \u00e9 uma homenagem ao famoso \"vira-lata caramelo\", um dos cachorros mais ic\u00f4nicos e queridos do Brasil. Esse nome ressoa com a ideia de resist\u00eancia, autenticidade e carisma \u2013 valores que tamb\u00e9m definem o esp\u00edrito da editora e suas hist\u00f3rias.\n</p>\n<p>Seja explorando o sobrenatural, a a\u00e7\u00e3o ou a fic\u00e7\u00e3o cient\u00edfica, a Caramelo Comics busca entregar HQs inovadoras e cativantes para os leitores apaixonados pelo universo dos quadrinhos independentes.\n</p>", "is_owner_or_admin": null, "newsletter": false, "subscribed_to_project_posts": true, "subscribed_to_new_followers": true, "subscribed_to_friends_contributions": true, "is_admin": false, "permalink": null, "email_active": null, "public_name": "Caramelo Comics", "following_this_user": false, "state_inscription": null, "birth_date": null, "account_type": null, "is_admin_role": false, "mail_marketing_lists": null}, "rewards": [{"id": 329614, "project_id": 160650, "description": "Livro f\u00edsico Blue Ghost", "minimum_value": 34.0, "maximum_contributions": null, "deliver_at": "2025-03-01T12:53:37", "updated_at": "2025-01-26T13:17:39.555524", "paid_count": 10, "waiting_payment_count": 0, "shipping_options": "national", "row_order": 0, "title": "Blue Ghost", "survey_sent_at": null, "survey_finished_at": null, "common_id": "6ae659ba-e14f-412b-a486-7e42fdb79232", "welcome_message_subject": "", "welcome_message_body": "", "uploaded_image": "https://s3-sa-east-1.amazonaws.com/cdn.br.catarse/uploads/reward/uploaded_image/329614/thumb_reward_ba5ef469-2dc6-443b-a2d4-25939287ba69.JPG", "run_out": false}]}